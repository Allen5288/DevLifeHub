{
    "name": "personal-website",
    "version": "1.0.0",
    "description": "Full stack personal website",
    "main": "index.js",
    "scripts": {
        "server": "cd server && npm run dev",
        "client": "cd client && npm start",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "install-all": "npm install && cd server && npm install && cd ../client && npm install",
        "build": "cd client && npm run build",
        "test": "concurrently \"cd server && npm test\" \"cd client && npm test\"",
        "lint": "concurrently \"cd server && npm run lint\" \"cd client && npm run lint\"",
        "format": "concurrently \"cd server && npm run format\" \"cd client && npm run format\"",
        "cypress": "cd client && npm run cypress:open",
        "clean": "rm -rf node_modules && cd server && rm -rf node_modules && cd ../client && rm -rf node_modules"
    },
    "keywords": [
        "react",
        "node",
        "express",
        "mongodb",
        "personal-website"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "aws-sdk": "^2.1001.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "express-mongo-sanitize": "^2.1.0",
        "express-rate-limit": "^6.0.0",
        "express-validator": "^6.14.0",
        "helmet": "^4.6.0",
        "hpp": "^0.2.3",
        "ioredis": "^4.28.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.0.12",
        "multer": "^1.4.3",
        "nodemailer": "^6.7.0",
        "passport": "^0.5.0",
        "passport-google-oauth20": "^2.0.0",
        "uuid": "^8.3.2",
        "winston": "^3.3.3",
        "xss-clean": "^0.1.1"
    },
    "devDependencies": {
        "concurrently": "^7.0.0"
    }
}